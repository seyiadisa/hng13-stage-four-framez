{
  "expo": {
    "name": "stage-four-framez",
    "slug": "stage-four-framez",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "stagefourframez",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#E6F4FE",
        "foregroundImage": "./assets/images/android-icon-foreground.png",
        "backgroundImage": "./assets/images/android-icon-background.png",
        "monochromeImage": "./assets/images/android-icon-monochrome.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false
    },
    "web": {
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff",
          "dark": {
            "backgroundColor": "#000000"
          }
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow Framez to access your photos to let you share them with your friends.",
          "cameraPermission": "Allow Framez to access your camera to let you take pictures."
        }
      ],
      [
        "expo-font",
        {
          "fonts": [
            {
              "fontFamily": "HKGrotesk",
              "fontDefinitions": [
                {
                  "path": "./assets/fonts/HKGrotesk-Regular.otf",
                  "weight": 400
                },
                {
                  "path": ".assets/fonts/HKGrotesk-Bold.otf",
                  "weight": 700
                }
              ]
            },
            {
              "fontFamily": "Allura",
              "fontDefinitions": [
                {
                  "path": "./assets/fonts/Allura-Regular.ttf",
                  "weight": 400
                }
              ]
            }
          ]
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    }
  }
}
